local Doffy_m=game:GetService("\067\111\114\101\071\117\105")local Doffy_g=game:GetService("\084\119\101\101\110\083\101\114\118\105\099\101")local Doffy_v=game:GetService("\080\108\097\121\101\114\115")local Doffy_u=game:GetService("\082\117\110\083\101\114\118\105\099\101")local Doffy_P=(game:GetService("\085\115\101\114\073\110\112\117\116\083\101\114\118\105\099\101")).TouchEnabled local Doffy_S=game:GetService("\082\117\110\083\101\114\118\105\099\101")local Doffy_C=workspace.CurrentCamera local Doffy_l=Doffy_v.LocalPlayer local Doffy_A=false local Doffy_c local Doffy_n local Doffy_j=function(Doffy_m,Doffy_v,Doffy_u)for Doffy_m,Doffy_P in Doffy_m do Doffy_P.Rotation=Doffy_v;(Doffy_g:Create(Doffy_P,TweenInfo.new(Doffy_u),{Rotation=0})):Play()end end local Doffy_x=function()return Doffy_l.Character:FindFirstChild("\072\117\109\097\110\111\105\100\082\111\111\116\080\097\114\116")or Doffy_l.Character.PrimaryPart end local Doffy_t=function()pcall(function()((Doffy_x()):FindFirstChild("\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\086\101\108\111\099\105\116\121")):Destroy();((Doffy_x()):FindFirstChild("\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\071\121\114\111")):Destroy();(Doffy_l.Character:FindFirstChildWhichIsA("\072\117\109\097\110\111\105\100")).PlatformStand=false Doffy_c:Disconnect()Doffy_n:Disconnect()Doffy_A=false end)end local Doffy_q=function()Doffy_t()Doffy_A=true local Doffy_m=Doffy_x()local Doffy_g=Instance.new("\066\111\100\121\071\121\114\111",Doffy_m)local Doffy_v=Instance.new("\066\111\100\121\086\101\108\111\099\105\116\121",Doffy_m)Doffy_v.Name="\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\086\101\108\111\099\105\116\121"Doffy_v.MaxForce=Vector3.zero Doffy_v.Velocity=Vector3.zero Doffy_g.P=1000 Doffy_g.D=50 Doffy_g.Name="\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\071\121\114\111"Doffy_g.MaxTorque=Vector3.new(9000000000,9000000000,9000000000)Doffy_c=Doffy_l.CharacterAdded:Connect(function()local Doffy_g=Instance.new("\066\111\100\121\071\121\114\111",Doffy_m)local Doffy_v=Instance.new("\066\111\100\121\086\101\108\111\099\105\116\121",Doffy_m)Doffy_v.Name="\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\086\101\108\111\099\105\116\121"Doffy_v.MaxForce=Vector3.zero Doffy_v.Velocity=Vector3.zero Doffy_g.P=1000 Doffy_g.D=50 Doffy_g.Name="\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\071\121\114\111"Doffy_g.MaxTorque=Vector3.new(9000000000,9000000000,9000000000)end)Doffy_n=Doffy_S.RenderStepped:Connect(function()if Doffy_l.Character:FindFirstChildWhichIsA("\072\117\109\097\110\111\105\100")and(Doffy_m and(Doffy_m:FindFirstChild("\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\086\101\108\111\099\105\116\121")and Doffy_m:FindFirstChild("\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\071\121\114\111")))then local Doffy_g=Doffy_l.Character:FindFirstChildWhichIsA("\072\117\109\097\110\111\105\100")local Doffy_v=Doffy_m:FindFirstChild("\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\086\101\108\111\099\105\116\121")local Doffy_u=Doffy_m:FindFirstChild("\068\111\102\102\097\110 \070\108\121 \083\104\105\116 \066\111\100\121\071\121\114\111")local Doffy_P=(require((Doffy_l.PlayerScripts:WaitForChild("\080\108\097\121\101\114\077\111\100\117\108\101")):WaitForChild("\067\111\110\116\114\111\108\077\111\100\117\108\101"))):GetMoveVector()Doffy_g.PlatformStand=true Doffy_v.MaxForce=Vector3.new(9000000000,9000000000,9000000000)Doffy_v.Velocity=Vector3.new()Doffy_u.MaxTorque=Vector3.new(9000000000,9000000000,9000000000)Doffy_u.CFrame=Doffy_C.CoordinateFrame if Doffy_P.X>0 then Doffy_v.Velocity=Doffy_v.Velocity+Doffy_C.CFrame.RightVector*(Doffy_P.X*50)end if Doffy_P.X<0 then Doffy_v.Velocity=Doffy_v.Velocity+Doffy_C.CFrame.RightVector*(Doffy_P.X*50)end if Doffy_P.Z>0 then Doffy_v.Velocity=Doffy_v.Velocity-Doffy_C.CFrame.LookVector*(Doffy_P.Z*50)end if Doffy_P.Z<0 then Doffy_v.Velocity=Doffy_v.Velocity-Doffy_C.CFrame.LookVector*(Doffy_P.Z*50)end end end)end local Doffy_h=Instance.new("\083\099\114\101\101\110\071\117\105",Doffy_m)local Doffy_a=Instance.new("\070\114\097\109\101",Doffy_h)local Doffy_H=Instance.new("\084\101\120\116\076\097\098\101\108",Doffy_a)local Doffy_Q=Instance.new("\084\101\120\116\066\117\116\116\111\110",Doffy_a)local Doffy_f=Instance.new("\084\101\120\116\076\097\098\101\108",Doffy_a)local Doffy_X=Instance.new("\084\101\120\116\076\097\098\101\108",Doffy_a)Doffy_X.Size=UDim2.new(1,0,0,20)Doffy_X.BackgroundTransparency=1 Doffy_X.TextScaled=true Doffy_X.RichText=true Doffy_X.Text="\067\114\101\100\105\116\115\058 \060\102\111\110\116 \099\111\108\111\114 \061 \"\035\048\048\102\102\102\102\"\062\068\111\102\102\097\110\060\047\102\111\110\116\062"Doffy_X.Position=UDim2.fromOffset(0,140)Doffy_X.TextColor3=Color3.fromRGB(255,255,255)Doffy_f.Position=UDim2.fromOffset(0,110)Doffy_f.BackgroundTransparency=1 Doffy_f.TextScaled=true Doffy_f.RichText=true Doffy_f.Text="\083\116\097\116\117\115\058 \060\102\111\110\116 \099\111\108\111\114 \061 \"\035\102\102\048\048\048\048\"\062\079\102\102\060\047\102\111\110\116\062"Doffy_f.Size=UDim2.new(1,0,0,30)Doffy_f.TextColor3=Color3.fromRGB(255,255,255)Doffy_Q.Position=UDim2.fromOffset(20,50)Doffy_Q.Text="\084\111\103\103\108\101"Doffy_Q.BackgroundColor3=Color3.fromRGB(10,100,200)Doffy_Q.BorderSizePixel=0 Doffy_Q.Size=UDim2.new(1,-40,0,50)Doffy_Q.TextScaled=true Doffy_Q.TextColor3=Color3.fromRGB(255,255,255)Doffy_H.TextScaled=true Doffy_H.Text="\083\105\109\112\108\101 \070\108\121 \086\048\046\049"Doffy_H.TextColor3=Color3.fromRGB(255,255,255)Doffy_H.BackgroundColor3=Color3.fromRGB(10,100,200)Doffy_H.Size=UDim2.new(1,0,0,30)Doffy_H.BorderSizePixel=0 Doffy_a.AnchorPoint=Vector2.new(.5,.5)Doffy_a.Position=UDim2.fromScale(.5,2)Doffy_a.Size=UDim2.fromOffset(300,160)Doffy_a.BorderSizePixel=3 Doffy_a.BorderColor3=Color3.fromRGB(20,200,200)Doffy_a.BackgroundColor3=Color3.fromRGB(20,20,20)Doffy_a.Selectable=true Doffy_a.Draggable=true Doffy_a.Active=true Doffy_h.ResetOnSpawn=false Doffy_h.IgnoreGuiInset=true Doffy_Q.MouseButton1Click:Connect(function()if not Doffy_A then Doffy_q()Doffy_f.Text="\083\116\097\116\117\115\058 \060\102\111\110\116 \099\111\108\111\114 \061 \"\035\048\048\102\102\048\048\"\062\079\110\060\047\102\111\110\116\062"else Doffy_t()Doffy_f.Text="\083\116\097\116\117\115\058 \060\102\111\110\116 \099\111\108\111\114 \061 \"\035\102\102\048\048\048\048\"\062\079\102\102\060\047\102\111\110\116\062"end end)wait(game.Loaded)Doffy_j({Doffy_a,Doffy_H,Doffy_Q,Doffy_f;Doffy_X},360,1);(Doffy_g:Create(Doffy_a,TweenInfo.new(.5),{Position=UDim2.fromScale(.5,.5)})):Play()

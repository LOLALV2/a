if Sex4.X > 0 then
Sex2.Velocity = Sex2.Velocity + PornHub7.CFrame.RightVector * (Sex4.X * (1 * 50))
end
if Sex4.X < 0 then
Sex2.Velocity = Sex2.Velocity + PornHub7.CFrame.RightVector * (Sex4.X * (1 * 50))
end
if Sex4.Z > 0 then
Sex2.Velocity = Sex2.Velocity - PornHub7.CFrame.LookVector * (Sex4.Z * (1 * 50))
end
if Sex4.Z < 0 then
Sex2.Velocity = Sex2.Velocity - PornHub7.CFrame.LookVector * (Sex4.Z * (1 * 50))
